<%- partial('_partial/head') %>
<body>
    <div id="container" :class="{'mobile-nav-on': mobileNavOn}">
        <%- partial('_partial/header', null, {cache: !config.relative_link}) %>
        <div class="skeleton">
            <img src="data:image/svg+xml;base64,">
            <img src="data:image/svg+xml;base64,">
            <img src="data:image/svg+xml;base64,">
        </div>
        <div class="wrap">
            <!-- 新html片段容器 -->
            <div class="content-wrapper">
                <keep-alive>
                    <router-view></router-view>
                </keep-alive>
            </div>
            <div class="main-outer" style="display: none">
                <% if (path === 'index.html' || /^page\/\d+\/index.html/.test(path)) { %>
                    <div class="theme-img-wrapper">
                        <% if (theme.banner) { %>

                            <% if (theme.banner.pc) { %>
                                <img src="<%= theme.banner.pc %>" class="theme-img theme-img-pc">
                            <% } %>

                            <% if (theme.banner.mobile) { %>
                                <img src="<%= theme.banner.mobile %>" class="theme-img theme-img-mobile">
                            <% } %>

                        <% } %>
                        <div class="main-info">
                            <h1><%= config.title %></h1>
                            <p class="summary"><%= config.subtitle %></p>
                        </div>
                    </div>
                <% } %>
                <div class="outer">
                    <section id="main" class="<%= path %>">
                        <%- body %>
                    </section>
                    <% if (theme.sidebar) { %>
                        <%- partial('_partial/sidebar', null, {cache: !config.relative_link}) %>
                    <% } %>
                </div>
            </div>
            <%- partial('_partial/footer', null, {cache: !config.relative_link}) %>
        </div>
        <%- partial('_partial/mobile-nav', null, {cache: !config.relative_link}) %>
        <%- partial('_partial/after-footer') %>
    </div>
</body>
</html>


